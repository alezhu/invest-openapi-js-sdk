    "prettier": "1.18.2",
    "rimraf": "^3.0.1",
    "rollup": "^1.21.3",
    "rollup-plugin-dts": "^1.1.11",
    "@rollup/plugin-typescript": "^3.0.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "tslib": "^1.10.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.10",
    "typedoc-plugin-no-inherit": "^1.1.10",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "ws": "^7.1.2"
