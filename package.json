{
  "name": "@tinkoff/invest-openapi-js-sdk",
  "version": "1.1.3",
  "description": "Tinkoff Invest OpenAPI Client",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
